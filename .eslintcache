leID(fileID)\r\n  }\r\n  // å…³é—­TabåŠŸèƒ½\r\n  const tabClose = (id) => {\r\n    // remove current id from openedFileID\r\n    const tabsWithout = openedFileIDs.filter(fileID => fileID !== id)\r\n    setOpenedFileIDs(tabsWithout)\r\n    // set the active to the first opened tab if still tabs left\r\n    if(tabsWithout.length > 0){\r\n      setActiveFileID(tabsWithout[0])\r\n    }else{\r\n      setActiveFileID('')\r\n    }\r\n  }\r\n  // å½“æ–‡ä»¶æ”¹å˜çš„æ—¶å€™\r\n  const fileChange = (id, value) => {\r\n    // åŸæ¥çš„ä»£ç \r\n    /* const newFile = files.map(file => {\r\n      if(file.id === id){\r\n      file.body = value\r\n    }\r\n    return file\r\n    }) */\r\n   \r\n   // å¦‚æœæ‰€ç¼–è¾‘çš„æ–‡ä»¶ä¸­æœ‰ä¿®æ”¹\r\n   if(value !== files[id].body){\r\n     // ä¿®æ”¹åçš„ä»£ç \r\n    const newFile = {...files[id], body: value}\r\n    setFiles({...files, [id]: newFile})\r\n    // update unsavedIDs\r\n    if(!unsavedFileIDs.includes(id)){\r\n      setUnsavedFileIDs([...unsavedFileIDs, id])\r\n    }\r\n   }\r\n  }\r\n    // å½“åˆ é™¤æ–‡ä»¶çš„æ—¶å€™\r\n  const deleteFile = (id) => {\r\n    fileHelper.deleteFile(files[id].path).then(() => {\r\n      // ä¿®æ”¹å‰çš„ä»£ç \r\n      //const newFile = files.filter(file => file.id !== id)\r\n      // ä¿®æ”¹åçš„ä»£ç \r\n      delete files[id]\r\n      setFiles(files)\r\n      // æ•°æ®æŒä¹…åŒ–\r\n      saveFilesToStore(files)\r\n      // close the tab if opened\r\n      tabClose(id)\r\n    })\r\n  }\r\n  // æ›´æ–°æ–‡ä»¶åçš„åŠŸèƒ½\r\n  const updateFileName = (id, title, isNew) => {\r\n    // ä¿®æ”¹å‰çš„ä»£ç \r\n    //  const newFiles = files.map(file => {\r\n    //    if(file.id === id){\r\n    //      file.title = title\r\n    //      file.isNew = false\r\n    //    }\r\n    //    return file\r\n    //  })\r\n    // setFiles(newFiles)\r\n    \r\n    // newPath should be different based on isNew\r\n    // if isNew is false, path should be old dirname + new title\r\n    const newPath = isNew ? \r\n      join(savedLocation, `${title}.md`)\r\n      : join(dirname(files[id].path), `${title}.md`)\r\n    \r\n    // ä¿®æ”¹åçš„ä»£ç \r\n    const modifiedFile = {...files[id], title, isNew: false, path: newPath}\r\n    const newFiles = { ...files, [id]: modifiedFile }\r\n    // å¦‚æœæ˜¯æ–°å»ºæ–‡ä»¶éœ€è¦ã€é¦–æ¬¡å‘½åã€‘\r\n    if(isNew){\r\n      // é‚£ä¹ˆä¿å­˜æ–°å»ºçš„æ–‡ä»¶åˆ°savedLocationæ‰€æŒ‡å®šçš„è·¯å¾„(å³documents)\r\n      fileHelper.writeFile(newPath,\r\n        files[id].body).then((res) => {\r\n          setFiles(newFiles)\r\n          saveFilesToStore(newFiles)\r\n          console.log(\"ğŸŒ¹ğŸŒ¹ğŸŒ¹ğŸŒ¹ğŸŒ¹ğŸŒ¹ğŸŒ¹ğŸŒ¹ğŸŒ¹æ‰§è¡Œäº†!\") // å¯ä»¥æ‰§è¡Œ\r\n          console.log(res) // undefined\r\n        }).catch(err => {\r\n          console.log(\"ğŸ‘»ğŸ‘»ğŸ‘»ğŸ‘»ğŸ‘»ğŸ‘»ğŸ‘»ğŸ‘»ğŸ‘»ğŸ‘»å¼‚å¸¸äº†!\")\r\n        })\r\n    }else{ //å¦åˆ™å¦‚æœæ˜¯å·²ç»å­˜åœ¨çš„æ–‡ä»¶éœ€è¦ã€é‡å‘½åã€‘\r\n      const oldPath = files[id].path\r\n      fileHelper.renameFile(oldPath,newPath).then(() => {\r\n        setFiles(newFiles)\r\n        saveFilesToStore(newFiles)\r\n      })\r\n    }\r\n  }\r\n  // æ–‡ä»¶æœç´¢åŠŸèƒ½\r\n  const fileSearch = (keyword) => {\r\n    // filter out the new files based on the keyword\r\n    \r\n    // ä¿®æ”¹å‰çš„ä»£ç \r\n    //const newFiles = files.filter(file => file.title.includes(keyword))\r\n    // ä¿®æ”¹åçš„ä»£ç \r\n    const newFiles = filesArr.filter(file => file.title.includes(keyword))\r\n    // setFiles(newFiles)\r\n    setSearchedFiles(newFiles)\r\n  }\r\n  \r\n  // æ–°å»ºæ–‡ä»¶\r\n  const createNewFile = () => {\r\n    const newID = uuidv4()\r\n    \r\n    // ä¿®æ”¹å‰çš„ä»£ç \r\n    //  const newFiles = [\r\n    //    ...files,\r\n    //    {\r\n    //      id: newID,\r\n    //      title: '',\r\n    //      body: '## è¯·è¾“å…¥Markdown',\r\n    //      createAt: new Date().getTime(),\r\n    //      isNew: true\r\n    //    }\r\n    //  ]\r\n    //  setFiles(newFiles)\r\n    \r\n    // ä¿®æ”¹åçš„ä»£ç \r\n    const newFile = {\r\n      id: newID,\r\n      title: '',\r\n  